package com.kavipriya;


public class Main {

    public static void main( String[] args) {
    
    int i,j;
    System.out.println("duplicate elements are:");
    int a[]= {1,2,3,4,6,3,2,1};
     for(i=0;i<a.length;i++){
      for(j=i+1;j<a.length;j++){
       if(a[i]==a[j])
        System.out.println(a[i]);
        }
      }
  }
}
